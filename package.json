{
  "name": "audio-snippet-detector",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist/**/*.js",
    "index.node",
    "index.node.d.ts"
  ],
  "scripts": {
    "test": "npm run test:node && npm run test:cargo",
    "test:cargo": "cargo test",
    "test:node": "tsc --noEmit && node --import tsx ts/**/*.test.ts",
    "cargo-build": "cargo build --message-format=json-render-diagnostics > cargo.log",
    "cross-build": "cross build --message-format=json-render-diagnostics > cross.log",
    "postcargo-build": "neon dist < cargo.log",
    "postcross-build": "neon dist -m /target < cross.log",
    "debug": "npm run cargo-build --",
    "build": "npm run build:node && npm run build:cargo",
    "build:node": "tsc --project tsconfig.dist.json",
    "build:cargo": "npm run cargo-build -- --release",
    "cross": "npm run cross-build -- --release",
    "prepack": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@neon-rs/cli": "0.1.82",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
